/**app.wxss**/
@import 'libs/weui/weui.wxss';
@import "colorui/main.wxss";
@import "colorui/icon.wxss";

.clear::after {
  clear: both;
  content: " ";
  display: table;
}

.image-box {
  display: inline-block;
  text-align: center;
}

.image-box::before {
  content: "";
  display: inline-block;
  vertical-align: middle;
  height: 100%;
}

.image-box image {
  max-height: 100%;
  max-width: 100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0rpx 0;
  box-sizing: border-box;
} 
